{
    "name": "sip/psinder-adoption",
    "type": "project",
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "path",
            "url": "../PhpSharedKernel",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "beberlei/assert": "^3.1",
        "bunny/bunny": "^0.4.1",
        "danielstjules/stringy": "^3.1",
        "doctrine/coding-standard": "^5.0",
        "doctrine/migrations": "^2.2",
        "filp/whoops": "^2.3",
        "guzzlehttp/guzzle": "^6.5",
        "guzzlehttp/psr7": "^1.6",
        "justinrainbow/json-schema": "^5.2",
        "laminas/laminas-component-installer": "^2.1",
        "laminas/laminas-config-aggregator": "^1.1",
        "laminas/laminas-dependency-plugin": "^1.0",
        "laminas/laminas-di": "^3.1",
        "laminas/laminas-diactoros": "^2.0",
        "laminas/laminas-servicemanager": "^3.3",
        "lcobucci/clock": "^1.3",
        "lstrojny/functional-php": "^1.8",
        "mezzio/mezzio": "^3.2",
        "mezzio/mezzio-fastroute": "^3.0",
        "mezzio/mezzio-helpers": "^5.1",
        "monolog/monolog": "^1.24",
        "nyholm/psr7": "^1.2",
        "php-ds/php-ds": "^1.2",
        "ramsey/uuid": "^3.8",
        "roave/psr-container-doctrine": "^2.1",
        "sip/psinder-php-sharedkernel": "@dev",
        "symfony/messenger": "^4.2",
        "symfony/property-access": "^4.2",
        "symfony/serializer": "^4.1",
        "xtreamwayz/expressive-messenger": "^1.0"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.3",
        "fzaninotto/faker": "^1.9",
        "helmich/phpunit-psr7-assert": "^4.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-beberlei-assert": "^0.12",
        "phpstan/phpstan-doctrine": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpstan/phpstan-strict-rules": "^0.12",
        "phpunit/phpunit": "^8.5",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Sip\\Psinder\\Adoption\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sip\\Psinder\\Adoption\\Test\\": "test/"
        }
    },
    "scripts": {
        "cs-check": "phpcs -s",
        "cs-fix": "phpcbf",
        "phpstan": "phpstan analyse -l max src test",
        "qa": [
            "@cs-check",
            "@phpstan",
            "@test-coverage"
        ],
        "test": "phpunit --colors=always",
        "test-coverage": "phpdbg -qrr vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "zf": {
            "component-whitelist": [
                "mezzio/mezzio",
                "mezzio/mezzio-helpers",
                "mezzio/mezzio-router",
                "laminas/laminas-httphandlerrunner"
            ]
        }
    }
}

{
    "name": "sip/psinder-php-sharedkernel",
    "type": "project",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "beberlei/assert": "^3.1",
        "doctrine/coding-standard": "^5.0",
        "lstrojny/functional-php": "^1.8",
        "php-ds/php-ds": "^1.2",
        "ramsey/uuid": "^3.8",
        "danielstjules/stringy": "^3.1",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "justinrainbow/json-schema": "^5.2",
        "phpstan/phpstan": "^0.10.5",
        "phpstan/phpstan-beberlei-assert": "^0.10.0",
        "phpstan/phpstan-phpunit": "^0.10.0",
        "phpstan/phpstan-strict-rules": "^0.10.1",
        "phpunit/phpunit": "^7.0.1",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.2",
        "symfony/messenger": "^4.2",
        "symfony/property-access": "^4.2",
        "symfony/serializer": "^4.1",
        "zendframework/zend-expressive-router": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Sip\\Psinder\\SharedKernel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sip\\Psinder\\SharedKernel\\Test\\": "test/"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "phpstan": "phpstan analyse -l max src test",
        "qa": [
            "@cs-check",
            "@phpstan",
            "@test-coverage"
        ],
        "test": "phpunit --colors=always",
        "test-coverage": "phpdbg -qrr vendor/bin/phpunit --colors=always"
    }
}
